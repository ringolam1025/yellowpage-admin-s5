{
  "collectionName": "components_shop_ads",
  "info": {
    "displayName": "Ads"
  },
  "options": {},
  "attributes": {
    "caption": {
      "type": "string"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "Discover 1A",
        "Discover 1B",
        "Discover 1C",
        "News 2A",
        "News Article 3A",
        "News Article 3B",
        "Events 4A",
        "Shops 5A",
        "Shop Category 6A",
        "Shop Category 6B"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "start": {
      "type": "date"
    },
    "end": {
      "type": "date"
    },
    "path_type": {
      "type": "enumeration",
      "enum": [
        "External",
        "Internal"
      ]
    },
    "path": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "path_type"
            },
            "External"
          ]
        }
      }
    },
    "shops": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::directory.directory",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "path_type"
            },
            "Internal"
          ]
        }
      }
    }
  },
  "config": {}
}
