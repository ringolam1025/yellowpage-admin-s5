{
  "kind": "collectionType",
  "collectionName": "directories",
  "info": {
    "singularName": "directory",
    "pluralName": "directories",
    "displayName": "Directory"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "premium_type": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "free",
      "enum": [
        "free",
        "tier_1",
        "tier_2"
      ]
    },
    "premium_start_date": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "conditions": {
        "visible": {
          "!=": [
            {
              "var": "premium_type"
            },
            "free"
          ]
        }
      },
      "required": false,
      "unique": false
    },
    "premium_end_date": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "conditions": {
        "visible": {
          "!=": [
            {
              "var": "premium_type"
            },
            "free"
          ]
        }
      },
      "required": false
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "directories"
    },
    "address": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "latitude": {
      "type": "float",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "longitude": {
      "type": "float",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "rating": {
      "type": "integer",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": false,
      "default": 5,
      "min": 0,
      "max": 5
    },
    "images": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "phone": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "phone2": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "about": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "review": {
      "type": "json",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "logo": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "opening_hours": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "shop.opening-hours",
      "repeatable": true,
      "required": false
    },
    "social_media": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "common.social-media",
      "repeatable": true
    },
    "rewards": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "shop.rewards",
      "repeatable": true
    }
  }
}
